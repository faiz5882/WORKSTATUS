<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			  xmlns:etc="http://schemas.microsoft.com/expression/2010/interactivity"
				xmlns:styles="clr-namespace:Material.Styles;assembly=Material.Styles"
				xmlns:user="clr-namespace:WorkStatus.Views;assembly=WorkStatus"
        xmlns:local="clr-namespace:WorkStatus.Utility"
		    xmlns:progRing="clr-namespace:AvaloniaProgressRing;assembly=AvaloniaProgressRing"
        mc:Ignorable="d"
		    Icon="avares://WorkStatus/Assets/header_Logo.png"
		    x:Class="WorkStatus.Views.Dashboard"  Title="Dashboard">
	<Window.Resources>
		<local:SubstringConverter x:Key="valueconverter"></local:SubstringConverter>
	</Window.Resources>
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="Auto"></ColumnDefinition>
			<ColumnDefinition Width="1"></ColumnDefinition>
			<ColumnDefinition Width="Auto"></ColumnDefinition>
		</Grid.ColumnDefinitions>

    <!-- DashBoard Add Task Alert PopUp while timer is runing-->
    <Popup IsOpen="{Binding IsAddTaskMode}" StaysOpen="{Binding IsAddTaskModeQuitAlert}" Margin="30" Height="140" Width="420" HorizontalOffset="360" VerticalOffset="400">
      <Border BoxShadow="0 0 0 gray" BorderThickness="0"  Height="120" Width="420" Background="WhiteSmoke" CornerRadius="5,5,5,5" Padding="5">
        <Grid>
          <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition Height="Auto"/>
          </Grid.RowDefinitions>
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
          </Grid.ColumnDefinitions>
          <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" BoxShadow="0 0 0 gray" BorderThickness="0"  Height="20" Width="420" Background="#A3A3A3" CornerRadius="5,5,0,0" Margin="0,-10,0,0" Padding="0">
            <StackPanel Orientation="Horizontal" Margin="0,0,0,0"  Width="420">
              <Image Margin="10,0,10,0" Source="/Assets/DotsIcon.png" VerticalAlignment="Center" HorizontalAlignment="Left"  Height="8"/>
              <TextBlock TextWrapping="Wrap" Margin="100,0,0,0"
							   TextAlignment="Center" Text="WorkStatus" Background="Transparent" HorizontalAlignment="Center"
							   FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
							   Foreground="{Binding NewTextColor,Mode=TwoWay}" FontStyle="Normal" FontSize="14"/>
            </StackPanel>
          </Border>
          <Grid Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2">
            <Grid.RowDefinitions>
              <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <!--<Image Grid.Row="0" Grid.Column="0" Margin="10,10,10,10" Source="/Assets/WorkStatuslogo_Minimal_1.png" VerticalAlignment="Center" HorizontalAlignment="Center"  Height="70"/>-->
            <Grid Grid.Row="0" Margin="10,0,10,20" HorizontalAlignment="Center">
              <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
              </Grid.RowDefinitions>

              <TextBlock Grid.Row="0" TextWrapping="Wrap" Text="While timer is runing you can not add task/edit task. Please, stop the Timer!" Foreground="{Binding NewTextColor,Mode=TwoWay}"   FontStyle="Normal" TextAlignment="Center" FontSize="14"  HorizontalAlignment="Center"  Margin="5,20,0,0" FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"/>
              <Button  x:Name="okAddTaskRunning" Grid.Row="2" Content="OK" Margin="10,10,15,10" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="14" FontWeight="SemiLight" Background="{Binding NewStackPanelLogoColor,Mode=TwoWay}" Foreground="White" Width="80" Height="30"/>
            </Grid>
          </Grid>
        </Grid>
      </Border>
    </Popup>
    
		<!-- Forcefully App Upgarde PopUp -->
		<Popup IsOpen="{Binding IsForceUpgradeApp}" StaysOpen="{Binding IsStayOpen}" Margin="30" Height="250" Width="300" HorizontalOffset="355" VerticalOffset="200">
			<Border BoxShadow="0 0 1 gray" BorderThickness="2" Height="250" Width="300" Background="WhiteSmoke" CornerRadius="8,8,8,8" Padding="5">
				<Grid>
					<Grid.RowDefinitions>
						<RowDefinition Height="1*"/>
						<RowDefinition Height="0.5*"/>
						<RowDefinition Height="1*"/>
						<RowDefinition Height="1*"/>
						<RowDefinition Height="1.5*"/>
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="1*"/>
						<ColumnDefinition Width="1*"/>
					</Grid.ColumnDefinitions>
					<Image Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,20,0,0" Source="/Assets/WorkStatus_logo.png" Height="50"/>
					<TextBlock Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" TextWrapping="Wrap" Text="{Binding AppTitle}" FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App" Foreground="#212529" FontWeight="Bold" FontStyle="Normal" TextAlignment="Center" FontSize="16" LineHeight="19"/>
					<TextBlock Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="2" TextWrapping="Wrap" TextAlignment="Center" Text="{Binding AppDescription}" FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App" Foreground="#212529" FontStyle="Normal" FontSize="14"/>
					<Button  x:Name="upgardeapp" Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="2"  Margin="20,0,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" Content="Download now"  FontSize="14" FontWeight="SemiLight" Background="#155fcc" Foreground="White" Width="200" Height="30"/>
				</Grid>
			</Border>
		</Popup>

		<!-- DashBoard sleep mode  Alert PopUp  Height="160"-->
		<Popup IsOpen="{Binding IsSleepMode}" StaysOpen="{Binding IsSleepModeQuitAlert}" Margin="30" Height="140" Width="420" HorizontalOffset="360" VerticalOffset="400">
			<Border BoxShadow="0 0 0 gray" BorderThickness="0"  Height="120" Width="420" Background="WhiteSmoke" CornerRadius="5,5,5,5" Padding="5">
				<Grid>
					<Grid.RowDefinitions>
						<RowDefinition Height="20"/>
						<RowDefinition Height="Auto"/>
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="1*"/>
						<ColumnDefinition Width="1*"/>
					</Grid.ColumnDefinitions>
					<Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" BoxShadow="0 0 0 gray" BorderThickness="0"  Height="20" Width="420" Background="#A3A3A3" CornerRadius="5,5,0,0" Margin="0,-10,0,0" Padding="0">
						<StackPanel Orientation="Horizontal" Margin="0,0,0,0"  Width="420">
							<Image Margin="10,0,10,0" Source="/Assets/DotsIcon.png" VerticalAlignment="Center" HorizontalAlignment="Left"  Height="8"/>
							<TextBlock TextWrapping="Wrap" Margin="100,0,0,0"
							   TextAlignment="Center" Text="WorkStatus" Background="Transparent" HorizontalAlignment="Center"
							   FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
							   Foreground="{Binding NewTextColor,Mode=TwoWay}" FontStyle="Normal" FontSize="14"/>
						</StackPanel>
					</Border>
					<Grid Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2">
						<Grid.RowDefinitions>
							<RowDefinition Height="Auto"/>
						</Grid.RowDefinitions>
						<!--<Image Grid.Row="0" Grid.Column="0" Margin="10,10,10,10" Source="/Assets/WorkStatuslogo_Minimal_1.png" VerticalAlignment="Center" HorizontalAlignment="Center"  Height="70"/>-->
						<Grid Grid.Row="0" Margin="10,0,10,20" HorizontalAlignment="Center">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto"/>
								<RowDefinition Height="Auto"/>
								<RowDefinition Height="Auto"/>
							</Grid.RowDefinitions>

							<TextBlock Grid.Row="0" TextWrapping="Wrap" Text="WorkStatus noticed that your system was on sleep mode. Please, Start the Timer again!" Foreground="{Binding NewTextColor,Mode=TwoWay}"   FontStyle="Normal" TextAlignment="Center" FontSize="14"  HorizontalAlignment="Center"  Margin="5,20,0,0" FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"/>
							<Button  x:Name="okConfirmApp" Grid.Row="2" Content="OK" Margin="10,10,15,10" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="14" FontWeight="SemiLight" Background="{Binding NewStackPanelLogoColor,Mode=TwoWay}" Foreground="White" Width="80" Height="30"/>
						</Grid>
					</Grid>
				</Grid>
			</Border>
		</Popup>


		<!-- DashBoard Close Alert PopUp  Height="160"-->
		<Popup IsOpen="{Binding IsDashBoardClose}" StaysOpen="{Binding IsDashBoardQuitAlert}" Margin="30" Height="120" Width="420" HorizontalOffset="360" VerticalOffset="400">
			<Border BoxShadow="0 0 0 gray" BorderThickness="0"  Height="120" Width="420" Background="WhiteSmoke" CornerRadius="5,5,5,5" Padding="5">
				<Grid>
					<Grid.RowDefinitions>
						<RowDefinition Height="20"/>
						<RowDefinition Height="Auto"/>
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="1*"/>
						<ColumnDefinition Width="1*"/>
					</Grid.ColumnDefinitions>
					<Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" BoxShadow="0 0 0 gray" BorderThickness="0"  Height="20" Width="420" Background="#A3A3A3" CornerRadius="5,5,0,0" Margin="0,-10,0,0" Padding="0">
						<StackPanel Orientation="Horizontal" Margin="0,0,0,0"  Width="420">
							<Image Margin="10,0,10,0" Source="/Assets/DotsIcon.png" VerticalAlignment="Center" HorizontalAlignment="Left"  Height="8"/>
							<TextBlock TextWrapping="Wrap" Margin="100,0,0,0"
							   TextAlignment="Center" Text="WorkStatus" Background="Transparent" HorizontalAlignment="Center"
							   FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
							   Foreground="{Binding NewTextColor,Mode=TwoWay}" FontStyle="Normal" FontSize="14"/>
						</StackPanel>
					</Border>
					<Grid Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2">
						<Grid.RowDefinitions>
							<RowDefinition Height="Auto"/>
						</Grid.RowDefinitions>
						<!--<Image Grid.Row="0" Grid.Column="0" Margin="10,10,10,10" Source="/Assets/WorkStatuslogo_Minimal_1.png" VerticalAlignment="Center" HorizontalAlignment="Center"  Height="70"/>-->
						<Grid Grid.Row="0" Margin="10,0,10,10" HorizontalAlignment="Center">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto"/>
								<RowDefinition Height="Auto"/>
								<RowDefinition Height="Auto"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto"/>
								<ColumnDefinition Width="Auto"/>
								<ColumnDefinition Width="Auto"/>
							</Grid.ColumnDefinitions>
							<TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" TextWrapping="Wrap" Text="Do you want to Quit Workstatus?" Foreground="{Binding NewTextColor,Mode=TwoWay}"   FontStyle="Normal" TextAlignment="Center" FontSize="14"  HorizontalAlignment="Center"  Margin="5,20,0,0" FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"/>
							<CheckBox x:Name="remembermecheckbox" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" IsVisible="False" Content="Remember my Choice" IsChecked="{Binding RemeberMe}" Margin="5,10,0,0" FontSize="14" FontStyle="Normal"/>
							<Button  x:Name="minimizeapp" Grid.Row="2" Grid.Column="0" Content="Minimize" Margin="0,10,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14" FontWeight="SemiLight" Background="{Binding NewStackPanelLogoColor,Mode=TwoWay}" Foreground="White" Width="80" Height="30"/>
							<Button  x:Name="quitapp" Grid.Row="2" Grid.Column="1" Content="Quit" Margin="10,10,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14" FontWeight="SemiLight" Background="{Binding NewStackPanelLogoColor,Mode=TwoWay}" Foreground="White" Width="80" Height="30"/>
							<Button  x:Name="cancelapp" Grid.Row="2" Grid.Column="2" Content="Cancel" Margin="10,10,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14" FontWeight="SemiLight" Background="{Binding NewStackPanelLogoColor,Mode=TwoWay}" Foreground="White" Width="80" Height="30"/>
						</Grid>
					</Grid>
				</Grid>
			</Border>
		</Popup>

		<!-- Idle Time Alert PopUp -->
		<Popup IsOpen="{Binding IsIdleTimeClose}" StaysOpen="{Binding IsIdleTimeQuitAlert}" Margin="30"
			   Height="400" Width="320" HorizontalOffset="360" PlacementMode="Top">
			<Border BoxShadow="0 0 1 gray" BorderThickness="0"  Height="400" Width="320" Background="White" CornerRadius="8,8,8,8" Padding="5">
				<Grid>
					<Grid.RowDefinitions>
						<RowDefinition Height="20"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="50"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="40"/>
						<RowDefinition Height="40"/>
						<RowDefinition Height="40"/>
					</Grid.RowDefinitions>
					<Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" BoxShadow="0 0 0 gray" BorderThickness="0"  Height="20" Width="320" Background="#A3A3A3" CornerRadius="5,5,0,0" Margin="0,-12,0,0" Padding="0">
						<StackPanel Orientation="Horizontal" Margin="0,0,0,0"  Width="320">
							<Image Margin="10,0,10,0" Source="/Assets/DotsIcon.png" VerticalAlignment="Center" HorizontalAlignment="Center"  Height="8"/>
							<TextBlock TextWrapping="Wrap" Margin="60,0,0,0"
							   TextAlignment="Center" Text="WorkStatus" Background="Transparent" HorizontalAlignment="Center"
							   FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
							   Foreground="{Binding NewTextColor,Mode=TwoWay}" FontStyle="Normal" FontSize="14"/>
						</StackPanel>
					</Border>

					<!--<Image Grid.Row="0" Margin="0,10,10,10" Source="/Assets/DotsIcon.png" VerticalAlignment="Top" HorizontalAlignment="Left"  Height="15"/>-->
					<TextBlock Grid.Row="1" Text="No Activity found" Margin="0,5,0,0" TextWrapping="Wrap" Foreground="Black" FontStyle="Normal" FontSize="14" FontWeight="Bold" TextAlignment="Center" FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"/>
					<TextBlock Grid.Row="2" Text="{Binding IdleTimeMessage}" Margin="65,5,0,0" 
                     TextWrapping="Wrap" Foreground="Black" TextAlignment="Left"
										 FontStyle="Normal" FontWeight="SemiBold" FontSize="14" 
										   FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"/>
					<CheckBox x:Name="IdleTimeCheckbox" Grid.Row="4" Content="Keep idle time" IsChecked="{Binding RemeberMeKeepIdle}" Margin="5,10,0,0" FontSize="14" FontStyle="Normal" HorizontalAlignment="Center" VerticalAlignment="Center"/>
					<Button  x:Name="continue" Grid.Row="5" Content="Continue" Command="{Binding CommandContinueIdleTime}" Margin="0,10,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14" FontWeight="SemiLight"  Background="{Binding NewStackPanelLogoColor,Mode=TwoWay}" Foreground="White" Width="280" Height="30"/>
					<Button  x:Name="stop" Grid.Row="6" Content="Stop" Command="{Binding CommandStopIdleTime}" Margin="0,10,0,0"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14" FontWeight="SemiLight"  Background="{Binding NewStackPanelLogoColor,Mode=TwoWay}" Foreground="White" Width="280" Height="30"/>
					<Button  x:Name="reassign" Grid.Row="7" Content="Reassign" Command="{Binding CommandReassignIdleTime}" Margin="0,10,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14" FontWeight="SemiLight"  Background="{Binding NewStackPanelLogoColor,Mode=TwoWay}" Foreground="White" Width="280" Height="30"/>

				</Grid>
			</Border>
		</Popup>

		<!--Reassign Idle Time PopUp  -->
		<Popup IsOpen="{Binding IsReassignBtnClick}" StaysOpen="{Binding IsReassignPopUpOpen}" Margin="30" Height="310" Width="370" PlacementMode="Top" HorizontalOffset="370" VerticalOffset="150">
			<Border BoxShadow="0 0 1 gray" BorderThickness="0"  Height="310" Width="370" Background="White" CornerRadius="8,8,8,8" Padding="5">
				<Grid>
					<Grid.RowDefinitions>
						<RowDefinition Height="100"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="60"/>
						<RowDefinition Height="50"/>
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="50"/>
						<ColumnDefinition Width="*"/>
					</Grid.ColumnDefinitions>
					<Image Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Margin="10,10,10,10" Source="/Assets/WorkStatuslogo_Minimal_1.png" VerticalAlignment="Center" HorizontalAlignment="Center"  Height="70"/>
					<TextBlock Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Text="ReAssign idle time" Margin="0,5,0,0" TextWrapping="Wrap" Foreground="Black" FontStyle="Normal" FontSize="14" FontWeight="Bold" TextAlignment="Center" FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"/>
					<TextBlock Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Text="{Binding IdleTimeMessage}" Margin="0,5,0,0" TextWrapping="Wrap" Foreground="Black" FontStyle="Normal" FontWeight="SemiBold" FontSize="14" TextAlignment="Center" FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"/>
					<Grid Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="2" Margin="30,15,0,0">
						<Grid.RowDefinitions>
							<RowDefinition Height="30"/>
							<RowDefinition Height="30"/>
						</Grid.RowDefinitions>
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="50"/>
							<ColumnDefinition Width="*"/>
						</Grid.ColumnDefinitions>
						<TextBlock Grid.Row="0" Grid.Column="0" Text="Project" TextWrapping="Wrap" Foreground="Black" FontStyle="Normal" FontWeight="SemiBold" FontSize="14" TextAlignment="Center" FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"/>
						<ComboBox x:Name="projectdropdown" Grid.Row="0" Grid.Column="1" Items="{Binding ProjectsList}" SelectedItem="{Binding IdleSelectedProject}" Height="28" Width="230"  Margin="10,0,0,0" >
							<ComboBox.ItemTemplate>
								<DataTemplate>
									<TextBlock Text="{Binding ProjectName}"/>
								</DataTemplate>
							</ComboBox.ItemTemplate>
						</ComboBox>
						<TextBlock Grid.Row="1" Grid.Column="0" Text="Todos" Margin="0,10,0,0" TextWrapping="Wrap" Foreground="Black" FontStyle="Normal" FontWeight="SemiBold" FontSize="14" TextAlignment="Center" FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"/>
						<ComboBox x:Name="tododropdown" Grid.Row="1" Grid.Column="1" Items="{Binding ToDoList}" SelectedItem="{Binding IdleSelectedProjectToDo}" Height="28" Width="230"  Margin="10,5,0,0" >
							<ComboBox.ItemTemplate>
								<DataTemplate>
									<TextBlock Text="{Binding ToDoName}"/>
								</DataTemplate>
							</ComboBox.ItemTemplate>
						</ComboBox>
					</Grid>
					<Grid Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,10,0,0">
						<Grid.RowDefinitions>
							<RowDefinition Height="Auto"/>
						</Grid.RowDefinitions>
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="70"/>
							<ColumnDefinition Width="Auto"/>
							<ColumnDefinition Width="Auto"/>
						</Grid.ColumnDefinitions>
						<Button x:Name="donereassign" Grid.Row="0" Grid.Column="1" Content="Assign" Command="{Binding CommandAssign}" Margin="0,10,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontWeight="SemiLight" Background="#155fcc" Foreground="White" Width="125" Height="30"/>
						<Button x:Name="cancelreassign" Grid.Row="0" Grid.Column="2" Content="Cancel" Command="{Binding CommandAssignCancel}" Margin="10,10,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontWeight="SemiLight" Background="#155fcc" Foreground="White" Width="125" Height="30"/>
					</Grid>
				</Grid>
			</Border>
		</Popup>

		<!-- Grid Left Start here-->
		<Grid x:Name="gridLeft" Grid.Row="0" Grid.Column="0" IsEnabled="{Binding IsAppBlock}" VerticalAlignment="Top">
			<Grid.RowDefinitions>
				<RowDefinition Height="200"/>
				<RowDefinition Height="400"/>
				<RowDefinition Height="30"/>
			</Grid.RowDefinitions>

			<Grid x:Name="TopLeftGrid" Grid.Row="0">
				<Grid.RowDefinitions>
					<RowDefinition Height="50"/>
					<RowDefinition Height="50"/>
					<RowDefinition Height="10"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
				</Grid.RowDefinitions>
				<Grid  Grid.Row="0" HorizontalAlignment="Center" Width="225" Margin="10,0,0,10">
					<Grid.RowDefinitions>
						<RowDefinition Height="50"/>
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="*"/>
						<ColumnDefinition Width="50"/>
					</Grid.ColumnDefinitions>
					<Image Grid.Row="0" Grid.Column="0" Source="/Assets/workstatuslogo.png" Height="40" VerticalAlignment="Bottom"/>
					<Image Grid.Row="0" Grid.Column="1" Source="/Assets/DotsIcon.png" Height="8" VerticalAlignment="Bottom"  HorizontalAlignment="Left" Margin="5,0,0,0"/>
				</Grid>
				<Grid Grid.Row="1" Margin="0,10,20,0">
					<Grid.RowDefinitions>
						<RowDefinition Height="Auto"/>
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="10"/>
						<ColumnDefinition Width="Auto"/>
						<ColumnDefinition Width="Auto"/>
						<ColumnDefinition Width="Auto"/>
					</Grid.ColumnDefinitions>
					<!--<Button x:Name="screenplay" HorizontalAlignment="Center" Grid.Row="0" Grid.Column="1"
                  Margin="20,0,0,0" IsVisible="{Binding IsPlaying,Mode=TwoWay}" 
                  Width="40" Height="40" Background="Transparent" Foreground="Transparent" 
                  Command="{Binding CommandPlay}"	>
					<Panel>
						<Image Width="40" Height="40" Source="/Assets/Group_8.png" />
					</Panel>
				</Button>-->
					<!--<Button Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" Margin="20,0,0,0"
          IsVisible="{Binding IsStop,Mode=TwoWay}" 
          Width="40" Height="40" Background="Transparent" 
          Foreground="Transparent" Command="{Binding CommandStop}"	>
					<Panel>
						<Image Width="40" Height="40" Source="/Assets/Pause.png"/>
					</Panel>
				</Button>-->
					<ToggleButton Grid.Row="0" Grid.Column="1" IsVisible="{Binding IsPlaying,Mode=TwoWay}" Command="{Binding CommandPlay}">
						<ToggleButton.Template>
							<ControlTemplate TargetType="ToggleButton">
								<Image  Width="40" Height="40"  Margin="5,0,0,0" Source="/Assets/StopIcon.png"/>
							</ControlTemplate>
						</ToggleButton.Template>
					</ToggleButton>

					<ToggleButton Grid.Row="0" Grid.Column="1" IsVisible="{Binding IsStop,Mode=TwoWay}" Command="{Binding CommandStop}">
						<ToggleButton.Template>
							<ControlTemplate TargetType="ToggleButton">
								<Image  Width="40" Height="40"  Margin="5,0,0,0" Source="/Assets/Pause1.png"/>
							</ControlTemplate>
						</ToggleButton.Template>
					</ToggleButton>

					<Border Grid.Row="0" Grid.Column="2" Width="225" Height="48" Background="{Binding NewStackPanelLogoColor,Mode=TwoWay}" Margin="10,0,0,0" CornerRadius="8" Padding="16">
						<TextBlock Text="{Binding HeaderTime}" Foreground="#FFFFFF" FontWeight="600" Width="90" FontSize="20" FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App" HorizontalAlignment="Center" VerticalAlignment="Center" />
					</Border>

					<StackPanel Grid.Row="0" Grid.Column="4" IsEnabled="{Binding IsAppBlock}" HorizontalAlignment="Center" Margin="10,0,0,0">
						<ToggleButton Name="addnotebtn" Opacity="{Binding AddNoteButtonOpacity}">
							<ToggleButton.Template>
								<ControlTemplate TargetType="ToggleButton">
									<Image  Width="30" Height="30" IsEnabled="{Binding IsAddNoteButtonEnabled}" Margin="0,8,0,0"  Source="/Assets/EditIcon.png"/>
								</ControlTemplate>
							</ToggleButton.Template>
						</ToggleButton>
						<Popup IsOpen="{Binding IsChecked, ElementName=addnotebtn}" StaysOpen="False" Margin="30" PlacementMode="Right" Height="250" Width="340" >
