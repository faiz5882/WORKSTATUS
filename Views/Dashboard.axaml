	xmlns:user="clr-namespace:WorkStatus.Views;assembly=WorkStatus"
		d:DesignWidth="800" d:DesignHeight="450"
        mc:Ignorable="d" Width="1000" Height="640"
		Icon="/Assets/avalonia-logo.ico"
        x:Class="WorkStatus.Views.Dashboard"
		CanResize="False"
		
        Title="Dashboard">
	<!--<UserControl>
		<Bitmap x:key="Play">Assets/Group_8.png</Bitmap>
		<Bitmap x:key="Pause">Assets/Group_9.png</Bitmap>
	</UserControl>-->
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="Auto"></ColumnDefinition>
			<ColumnDefinition Width="1"></ColumnDefinition>
			<ColumnDefinition Width="Auto"></ColumnDefinition>
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"></RowDefinition>
			<RowDefinition Height="Auto"></RowDefinition>
		</Grid.RowDefinitions>
		<!-- grdLeft Start here-->
		<Grid x:Name="gridLeft"
			  Grid.Row="0" Grid.Column="0"
			  VerticalAlignment="Top">
			<Grid.RowDefinitions>
				<RowDefinition Height="Auto"></RowDefinition>
				<RowDefinition Height="Auto"></RowDefinition>
				<RowDefinition Height="Auto"></RowDefinition>
				<RowDefinition Height="Auto"></RowDefinition>
				<RowDefinition Height="Auto"></RowDefinition>
				<RowDefinition Height="Auto"></RowDefinition>
				<RowDefinition Height="Auto"></RowDefinition>
				<RowDefinition Height="Auto"></RowDefinition>
				<RowDefinition Height="Auto"></RowDefinition>

				<RowDefinition Height="*"></RowDefinition>
			</Grid.RowDefinitions>
			<Image Grid.Row="0"
				   Source="/Assets/WorkStatus_logo.png"
								Height="50"></Image>
			<Grid Grid.Row="3" Margin="20,10,20,0">
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto"></RowDefinition>
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto"></ColumnDefinition>
					<ColumnDefinition Width="Auto"></ColumnDefinition>
					<ColumnDefinition Width="Auto"></ColumnDefinition>
				</Grid.ColumnDefinitions>
				<Button Grid.Row="0" Grid.Column="0"
						Background="Transparent"
						Foreground="Transparent"
					Command="{Binding CommandPlay}"	>
					<Panel>
						<!--<Image Width="48" Height="48" Source="{DynamicResource Play" IsVisible="{Binding !IsPlaying}"></Image>-->						
						<!--<Image Width="48" Height="48" Source="{DynamicResource Pause" IsVisible="{Binding !IsPlaying}"></Image>-->

					</Panel>
				</Button>
				

				<Border Width="208" Grid.Row="0" Grid.Column="1"
						Height="48"
						Background="#1665D8" Margin="10,0,0,0"
				  CornerRadius="8"
				  Padding="16">
					<TextBlock VerticalAlignment="Center"
							   Foreground="#FFFFFF" 
							   FontWeight="600"
							   FontSize="20"
							    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
							   HorizontalAlignment="Center">07:03:45</TextBlock>
				</Border>
				<Image Grid.Row="0" Grid.Column="3" Width="48"
					   Height="48" Margin="10,0,0,0"
					   Source="/Assets/Group_9.png"></Image>

			</Grid>
			<Grid Grid.Row="4" HorizontalAlignment="Center">
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto"></RowDefinition>
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto"></ColumnDefinition>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Row="0"
						   Text="WS_p2"
						    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
						Foreground="#212529"
					FontWeight="Bold" FontStyle="Normal"
					FontSize="16" LineHeight="19"
				Margin="0,10,0,0">

				</TextBlock>
			</Grid>
			<Grid Grid.Row="5" HorizontalAlignment="Center">
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto"></RowDefinition>
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto"></ColumnDefinition>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Row="0"
						   FontWeight="500" Foreground="Blue"
									   FontSize="16"
						    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
						   Text="No Weekly Limit" Margin="30,10,0,0"></TextBlock>
			</Grid>
			<Grid Grid.Row="6" HorizontalAlignment="Center">
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto"></RowDefinition>
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto"></ColumnDefinition>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Row="0"
						   FontWeight="Bold" FontStyle="Normal"
				FontSize="16"
						    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
						   Text="Total worked today : 07:30"
						   Margin="0,10,0,0"></TextBlock>
			</Grid>
			<Grid Grid.Row="7"  Margin="0,10,0,0">
				<Grid.RowDefinitions>
					<RowDefinition Height="*"></RowDefinition>
				</Grid.RowDefinitions>
				<Border  Grid.Row="0" Width="270"
                    BorderBrush="#E2E2E2"
            BorderThickness="2"
                         Background="#F7F7F7"
                    HorizontalAlignment="Stretch"
            VerticalAlignment="Center"
            CornerRadius="5">
					<StackPanel Margin="1"
				Orientation="Horizontal">

						<TextBox BorderBrush="Transparent"   Height="25"
								 FontSize="14" Width="270" HorizontalAlignment="Stretch"
								 VerticalAlignment="Stretch"  >
							<!--<TextBox.Template>
								<ControlTemplate TargetType="{x:Type TextBox}">
									<Grid Background="#F7F7F7">
										<Grid.ColumnDefinitions>
											<ColumnDefinition Width="*"/>
											<ColumnDefinition Width="30"/>
										</Grid.ColumnDefinitions>
									</Grid>
								</ControlTemplate>
							</TextBox.Template>
							<TextBox.Template>
								<ControlTemplate TargetType="{x:Type TextBox}">
									<Grid Background="#F7F7F7">
										<Grid.ColumnDefinitions>
											<ColumnDefinition Width="*"/>
											<ColumnDefinition Width="30"/>
										</Grid.ColumnDefinitions>
										<ScrollViewer Grid.Column="0" />										
										<Button Cursor="Hand" Margin="0,0,0,0" Grid.Column="1"  
												HorizontalAlignment="Right" Height="20" Width="30"
												 >
											<Button.Template>
												<ControlTemplate>
													<Border x:Name="theBorder" BorderBrush="Transparent" BorderThickness="0">
														<Image  Source="/Assets/button_blue_play.png"  Width="20" Height="20" HorizontalAlignment="Center"/>
													</Border>
												</ControlTemplate>
											</Button.Template>
										</Button>
									</Grid>
								</ControlTemplate>
							</TextBox.Template>-->
						</TextBox>
					</StackPanel>
				</Border>
			</Grid>
			<Grid Grid.Row="8" Margin="0,10,0,0" 
				  Background="#E8F3FF">
				<Grid.RowDefinitions>
					<RowDefinition Height="30"></RowDefinition>
					<RowDefinition Height="*"></RowDefinition>
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto"></ColumnDefinition>
					<ColumnDefinition Width="1.5*"></ColumnDefinition>
					<ColumnDefinition Width="Auto"></ColumnDefinition>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Row="0" Grid.Column="0"
						   VerticalAlignment="Center"
						   Margin="8,10,0,0"
						   Foreground="Black"
						   FontSize="16"
						    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
						   FontWeight="Bold"
						   Text="Ws_Org">
				</TextBlock>
				
					<!--<TextBlock Grid.Row="0" Grid.Column="2"
						   HorizontalAlignment="Right"
							   Margin="0,10,10,0"
							    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
							   VerticalAlignment="Center"
						  FontWeight="Bold" FontStyle="Normal"
					     FontSize="16" Foreground="#1665D8"
						   Text="Change"></TextBlock>-->
				
				<!-- Organisation Details popup-->
				<StackPanel Grid.Row="0" Grid.Column="2" Margin="0,10,0,0"
							HorizontalAlignment="Right" VerticalAlignment="Center">
					<ToggleButton Name="button">
						<ToggleButton.Template>
							<ControlTemplate TargetType="ToggleButton">
								<TextBlock HorizontalAlignment="Right"
													   Margin="0,0,10,0" Background="Transparent"
													   VerticalAlignment="Center"
												  FontWeight="Bold" FontStyle="Normal"
												 FontSize="16" Foreground="#1665D8">Change</TextBlock>
							</ControlTemplate>
						</ToggleButton.Template>
					</ToggleButton>
					<Popup IsOpen="{Binding IsChecked, ElementName=button}"
StaysOpen="False" PlacementMode="Right"
Height="300" Width="150" >
						<Border BoxShadow="0 0 1 gray" BorderThickness="2" Height="300" Width="150" Background="White"
						CornerRadius="8,8,8,8" Padding="5">
							<ListBox Background="White" SelectionMode="Single" SelectedItem="{Binding SelectedOrganisationItems,Mode=TwoWay}" ScrollViewer.VerticalScrollBarVisibility="Auto"
							Items="{Binding FindOrganisationDetails,Mode=TwoWay}">
								<ListBox.ItemTemplate>
									<DataTemplate>
										<Grid Background="LightGray">
											<Grid.RowDefinitions>
												<RowDefinition Height="30"></RowDefinition>
											</Grid.RowDefinitions>
											<Grid.ColumnDefinitions>
												<ColumnDefinition Width="150"></ColumnDefinition>
											</Grid.ColumnDefinitions>
											<TextBlock Grid.Row="0"
											Grid.Column="0" Text="{Binding OrganizationName}"
											HorizontalAlignment="Stretch"
											VerticalAlignment="Center"></TextBlock>
										</Grid>
									</DataTemplate>
								</ListBox.ItemTemplate>


							</ListBox>
						</Border>
					</Popup>

				</StackPanel>
				<Grid Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3"
					  Margin="-5,10,0,0" Height="255">
					<ListBox Background="White" SelectionMode="Single"  ScrollViewer.HorizontalScrollBarVisibility="Auto" 
							 Items="{Binding FindUserProjectList,Mode=TwoWay}">												
						<ListBox.ItemTemplate>
							<DataTemplate>																	
								<StackPanel Orientation="Horizontal">
									<Grid>
										<Grid.RowDefinitions>
											<RowDefinition Height="Auto"></RowDefinition>											
										</Grid.RowDefinitions>
										<Grid.ColumnDefinitions>
											<ColumnDefinition Width="10"></ColumnDefinition>
											<ColumnDefinition Width="100"></ColumnDefinition>
											<ColumnDefinition Width="230"></ColumnDefinition>											
										</Grid.ColumnDefinitions>
										<Image  Grid.Row="0" Margin="10,0,0,0"
												Grid.Column="0"
											   Source="/Assets/Group_8.png"
											 Width="25" Height="25"
											 HorizontalAlignment="Left" />
										<TextBlock Grid.Row="0" Grid.Column="1"
												   FontStyle="Normal"
												  FontWeight="Normal"
												   HorizontalAlignment="Center"
												   VerticalAlignment="Center" 
												    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
												   Text="{Binding ProjectName}"
												   FontSize="16"></TextBlock>
										
											<TextBlock  Grid.Row="0" Grid.Column="2"
												    Text="{Binding ProjectId}"
														 FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
												   HorizontalAlignment="Right"
												   VerticalAlignment="Center"></TextBlock>
										
										
									</Grid>
									<Separator/>
								</StackPanel>
								
							</DataTemplate>
						</ListBox.ItemTemplate>						
						
					</ListBox>
				</Grid>
			</Grid>
			<Grid Grid.Row="9" Margin="0,60,0,0" Background="#1665D8">
				<Grid.RowDefinitions>
					<RowDefinition Height="*"></RowDefinition>
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="*"></ColumnDefinition>
					<ColumnDefinition Width="*"></ColumnDefinition>
					<ColumnDefinition Width="*"></ColumnDefinition>
					<ColumnDefinition Width="*"></ColumnDefinition>
				</Grid.ColumnDefinitions>
				<Image Grid.Row="0"
				   Grid.Column="0"
				   Source="/Assets/refresh.png"
								Height="20"></Image>
				<Label Grid.Row="0" Grid.Column="1"
					   FontStyle="Normal"
					   FontSize="14"
					   FontWeight="400"
					   Foreground="#FFFFFF"
						 FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
					   VerticalContentAlignment="Center"
					   HorizontalContentAlignment="Center"
					   Content="Last updated at: 06/08/20"></Label>
				<Image Grid.Row="0"
				   Grid.Column="2" Margin="80,0,0,0"
				   Source="/Assets/maximize.png"
								Height="20"></Image>
				<Button Grid.Row="0" Grid.Column="3"
						Foreground="#1665D8" Background="#1665D8">
					<Image Grid.Row="0"
				   Grid.Column="3"
				   Source="/Assets/arrowhead-right.png"
								Height="20"></Image>
				</Button>


			</Grid>
		</Grid>
		<Grid x:Name="grdCenter"
			  Grid.Row="0" Grid.Column="1">
			<Rectangle  Fill="WhiteSmoke" Height="800"/>
		</Grid>
		<!-- grdRight Start here-->
		<Grid x:Name="grdRight" Grid.Row="0"
			  Margin="-15,0,0,0" Grid.Column="2">
			<Grid.RowDefinitions>
				<RowDefinition Height="*"></RowDefinition>
				<RowDefinition Height="*"></RowDefinition>
			</Grid.RowDefinitions>
			<Grid Grid.Row="0">
				<Grid.RowDefinitions>
					<RowDefinition Height="*"></RowDefinition>
					<RowDefinition Height="*"></RowDefinition>
					<RowDefinition Height="*"></RowDefinition>					

				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="100"></ColumnDefinition>
					<ColumnDefinition Width="*"></ColumnDefinition>
					<ColumnDefinition Width="*"></ColumnDefinition>
					
				</Grid.ColumnDefinitions>
			
			<TextBlock Grid.Row="0"
					   Grid.Column="0" 
					   Grid.ColumnSpan="3"
						 Text="WS_p2" 
					    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
					   VerticalAlignment="Center"
									Foreground="#212529"
								FontWeight="Bold" FontStyle="Normal"
								FontSize="16" LineHeight="19"
							Margin="30,10,0,0"></TextBlock>				
				<Border  Grid.Row="1" Grid.Column="0" Width="255"
                    BorderBrush="#E2E2E2" Margin="30,20,0,0"
            BorderThickness="2"
                         Background="#F7F7F7"
                    HorizontalAlignment="Left"
            VerticalAlignment="Center" 
            CornerRadius="5">
					<StackPanel Margin="1"
				Orientation="Horizontal">

						<TextBox BorderBrush="Transparent"   Height="25"
								 FontSize="14" Width="250" HorizontalAlignment="Stretch"
								 VerticalAlignment="Stretch"  >
							
						</TextBox>
					</StackPanel>
				</Border>
				<ComboBox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" 
						  VerticalAlignment="Center"  FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
						  Name="ComboBox1" Width="120" 
						  Height="30" SelectedIndex="0"
						  HorizontalAlignment="Left" Margin="0,20,0,0">
					<ComboBoxItem Content="All Todo's"></ComboBoxItem>					
				</ComboBox>
				<!-- To-Do Start here-->
				<Grid Margin="20"
					  Height="486" Grid.Row="2"
					  Grid.Column="0" Grid.ColumnSpan="2">
					<Grid.RowDefinitions>
						<RowDefinition Height="Auto"></RowDefinition>
						<RowDefinition Height="*"></RowDefinition>					
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="30"></ColumnDefinition>
						<ColumnDefinition Width="250"></ColumnDefinition>					
						<ColumnDefinition Width="120"></ColumnDefinition>					
						<ColumnDefinition Width="120"></ColumnDefinition>					
						<ColumnDefinition Width="120"></ColumnDefinition>					
						<ColumnDefinition Width="*"></ColumnDefinition>																					
					</Grid.ColumnDefinitions>
					<!-- To-Do Header-->
					<TextBlock Grid.Row="0" Grid.Column="1"						 						  
						   Foreground="Black"
						   FontSize="16"
						    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
						   FontWeight="Bold"
						   Text="To-do"></TextBlock>
				
				<TextBlock Grid.Row="0" Grid.Column="2"						 						  
						   Foreground="Black"
						   FontSize="16"
						    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
						   FontWeight="Bold"
						   Text="Start Date"></TextBlock>
				
				<TextBlock Grid.Row="0" Grid.Column="3"						 						  
						   Foreground="Black"
						   FontSize="16"
						    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
						   FontWeight="Bold"
						   Text="End Date"></TextBlock>
				
				<TextBlock Grid.Row="0" Grid.Column="4"						 						  
						   Foreground="Black"
						   FontSize="16"
						    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
						   FontWeight="Bold"
						   Text="Duration"></TextBlock>
					<!-- To-Do List-->
					<ListBox Grid.Row="1" VerticalAlignment="Top" 
							 Grid.ColumnSpan="5" 
							 Background="White" SelectionMode="Single"  
							 ScrollViewer.HorizontalScrollBarVisibility="Hidden"
						 Items="{Binding FindOrganisationDetails,Mode=TwoWay}">
						<ListBox.ItemTemplate>
							<DataTemplate>
								<StackPanel Orientation="Horizontal">
									<Grid>
										<Grid.RowDefinitions>
											<RowDefinition Height="*"></RowDefinition>
										</Grid.RowDefinitions>
										<Grid.ColumnDefinitions>
											<ColumnDefinition Width="20"></ColumnDefinition>
											<ColumnDefinition Width="250"></ColumnDefinition>
											<ColumnDefinition Width="120"></ColumnDefinition>
											<ColumnDefinition Width="120"></ColumnDefinition>
											<ColumnDefinition Width="50"></ColumnDefinition>
											<ColumnDefinition Width="*"></ColumnDefinition>
										</Grid.ColumnDefinitions>
										<Image  Grid.Row="0" Margin="-5,0,0,0"
												Grid.Column="0"
											   Source="/Assets/Group_8.png"
											 Width="25" Height="25"
											 HorizontalAlignment="Left" />										
										<Grid Grid.Row="0" Grid.Column="1">
											<Grid.RowDefinitions>
												<RowDefinition Height="*"></RowDefinition>
											</Grid.RowDefinitions>
											<Grid.ColumnDefinitions>
												<ColumnDefinition Width="*"></ColumnDefinition>
												<ColumnDefinition Width="Auto"></ColumnDefinition>
												<ColumnDefinition Width="Auto"></ColumnDefinition>
											</Grid.ColumnDefinitions>
											<TextBlock Grid.Row="0" Grid.Column="0"
												   FontStyle="Normal"
												  FontWeight="Normal"
													   Foreground="#1665D8"
												   HorizontalAlignment="Left"
												   VerticalAlignment="Center"
												    FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
												   Text="Wireframe 1 test data by inder manral" TextTrimming="WordEllipsis" TextWrapping="Wrap" Padding="10"
												   FontSize="16"></TextBlock>

											<Image  Grid.Row="0" 
												Grid.Column="1" VerticalAlignment="Center"
											   Source="/Assets/icon.png"
											 Width="17" Height="17"
											 HorizontalAlignment="Left" />
											<!--<Image  Grid.Row="0" VerticalAlignment="Center"
												Grid.Column="2"
											   Source="/Assets/icon.png"
											 Width="17" Height="17"
											 HorizontalAlignment="Left" />-->
											
											<Border Width="58" Grid.Row="0" Grid.Column="2"
													Height="17"
													Background="#0AA7FF" 
													CornerRadius="2"
													Padding="2">
												<TextBlock VerticalAlignment="Center"
														   Foreground="#FFFFFF"
														   FontWeight="500"
														   Opacity="0.9"
														   FontSize="14"
															FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
														   HorizontalAlignment="Center">On-site</TextBlock>
											</Border>
										</Grid>

										<TextBlock  Grid.Row="0" Grid.Column="2"
												Text="06/08/2020" Margin="4,0,0,0"
													 FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
											   HorizontalAlignment="Left"
											   VerticalAlignment="Center"></TextBlock>
										<TextBlock  Grid.Row="0" Grid.Column="3"
												Text="05/08/2020" Margin="4,0,0,0"
											   HorizontalAlignment="Left"
									   VerticalAlignment="Center"></TextBlock>
										<TextBlock  Grid.Row="0" Grid.Column="4"
												Text="00:00:00"  Width="60"
													 FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
											   HorizontalAlignment="Left"
											   VerticalAlignment="Center"></TextBlock>
										<TextBlock  Grid.Row="0"
													Grid.Column="5" Margin="30,0,0,0"
													Text="...." 
													FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
													 HorizontalAlignment="Right"
													 VerticalAlignment="Center"></TextBlock>
									</Grid>
									<Separator/>
								</StackPanel>

							</DataTemplate>
						</ListBox.ItemTemplate>

					</ListBox>	
					</Grid>
				<!-- To-Do End here-->

			</Grid>
			<!-- Footer Start here-->
			<Grid Grid.Row="1" Width="710" Margin="1,0,0,0"
				  Background="#1665D8">
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto"></RowDefinition>
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="520"></ColumnDefinition>
					<ColumnDefinition Width="Auto"></ColumnDefinition>
				</Grid.ColumnDefinitions>
				
				<Label Grid.Row="0" Grid.Column="0"
		   FontStyle="Normal"
		   FontSize="14"  Foreground="#FFFFFF" Margin="2"
						 FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"
		   FontWeight="400" 
		   VerticalContentAlignment="Center"
		   Content="Showing 2 of 2 tasks"></Label>
				<Label Grid.Row="0" Grid.Column="1"
					   FontStyle="Normal"
					   FontSize="14"
					  Margin="2"
					   FontWeight="400"
					    Foreground="#FFFFFF"
						 FontFamily="resm:WorkStatus.Assets.Fonts?assembly=WorkStatus#Airbnb Cereal App"					   
					   VerticalContentAlignment="Center"
					   HorizontalContentAlignment="Right"
					   Content="Version 1.8(865)"></Label>
			</Grid>
			<!-- Footer End here-->
	</Grid>



	</Grid>
</Window>
